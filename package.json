{
  "name": "slodka-petelka",
  "version": "1.0.0",
  "description": "Slodka Petelka",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "ci": "payload migrate && pnpm build",
    "build:with-migrations": "pnpm run payload migrate && cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.38.0",
    "@payloadcms/email-nodemailer": "^3.38.0",
    "@payloadcms/next": "^3.38.0",
    "@payloadcms/plugin-form-builder": "^3.38.0",
    "@payloadcms/richtext-lexical": "^3.38.0",
    "@react-hook/window-size": "^3.1.1",
    "@tanstack/react-query": "^5.83.0",
    "@types/nodemailer": "^6.4.17",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "graphql": "^16.8.1",
    "lenis": "^1.2.3",
    "license-checker": "^25.0.1",
    "monaco-editor": "^0.52.2",
    "motion": "^12.5.0",
    "next": "15.2.3",
    "nodemailer": "^6.10.0",
    "normalize-wheel": "^1.0.1",
    "payload": "latest",
    "picomatch": "^4.0.2",
    "pnpm": "^9.15.9",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "^5.5.0",
    "react-use": "^17.6.0",
    "sharp": "0.32.6",
    "yjs": "^13.6.27",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.2.2",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "packageManager": "pnpm@9.15.9"
}
